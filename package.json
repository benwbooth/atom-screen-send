{
  "name": "screen-send",
  "main": "./lib/screen-send",
  "version": "1.9.0",
  "description": "Send text to iTerm2, Terminal.app, Konsole, Tmux or Screen sessions",
  "keywords": [
    "terminal",
    "iterm2",
    "screen",
    "konsole",
    "tmux",
    "send"
  ],
  "activationCommands": {
    "atom-workspace": [
      "screen-send:list",
      "screen-send:send"
    ]
  },
  "repository": "https://github.com/benwbooth/atom-screen-send",
  "license": "MIT",
  "engines": {
    "atom": ">=1.0.0 <2.0.0"
  },
  "dependencies": {
    "atom-space-pen-views": "latest",
    "temp": "latest"
  }
}
